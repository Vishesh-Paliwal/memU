{
  "name": "n8n-nodes-memu",
  "version": "1.0.0",
  "description": "n8n nodes for MemU agentic memory integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "memu",
    "memory",
    "ai",
    "agent"
  ],
  "license": "MIT",
  "homepage": "https://github.com/memu-ai/n8n-nodes-memu",
  "author": {
    "name": "MemU Team",
    "email": "support@memu.so"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/memu-ai/n8n-nodes-memu.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=7.18"
  },
  "packageManager": "pnpm@8.6.2",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes/**/*.ts credentials/**/*.ts",
    "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MemUCloudApi.credentials.js",
      "dist/credentials/MemUSelfHostedApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MemU/MemUMemorize.node.js",
      "dist/nodes/MemU/MemURetrieve.node.js",
      "dist/nodes/MemU/MemUListItems.node.js",
      "dist/nodes/MemU/MemUCreateItem.node.js",
      "dist/nodes/MemU/MemUUpdateItem.node.js",
      "dist/nodes/MemU/MemUDeleteItem.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.2.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}